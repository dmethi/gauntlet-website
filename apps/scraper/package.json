{
  "name": "@gauntlet/scraper",
  "version": "1.0.0",
  "private": true,
  "description": "Data scraping and ingestion service for fantasy football statistics",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf dist",
    "scrape:players": "tsx src/scrapers/player-scraper.ts",
    "scrape:stats": "tsx src/scrapers/stats-scraper.ts",
    "scrape:schedule": "tsx src/scrapers/schedule-scraper.ts"
  },
  "dependencies": {
    "@gauntlet/types": "workspace:*",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.7",
    "puppeteer": "^22.4.0",
    "cron": "^3.1.6",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cron": "^2.4.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
} 